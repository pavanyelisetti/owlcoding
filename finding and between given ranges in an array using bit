#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    vector<vector<int>>v;
    for(int i=0;i<n;i++)
    {
        vector<int>t;
        for(int j=0;j<32;j++)
        {
            if(a[i]&(1<<j)){
                t.push_back(1);
            }
            else{
                t.push_back(0);
            }
        }
        v.push_back(t);
        t.clear();
    }
    for(int i=1;i<v.size();i++)
    {
        for(int j=0;j<v[i].size();j++)
        {
            v[i][j]=v[i][j]+v[i-1][j];
        }
    }
    for(int i=0;i<v.size();i++)
    {
        for(int j=0;j<v[i].size();j++)
        {
            cout<<v[i][j]<<" ";
        }
        cout<<endl;
    }
    int q;
    cin>>q;
    while(q--){
    int l,r,ans=0,res;
    cin>>l>>r;
    for(int i=0;i<32;i++){
        if(l!=0){
            res=v[r][i]-v[l-1][i];
            if(res==(r-l+1)){
            	ans+=(1<<i);
            }
        }
        else{
            res=v[r][i];
            if(v[r][i]==(r+1)){
                ans+=(1<<i);
            }
        }
       
        
    }
    cout<<ans<<endl;
    }

    
}
